{
  "manifest_version": 2,

  "name": "open with nhentai",
  "description": "open nhentai from code",
  "version": "1.0",

  "permissions": [
    "activeTab",
	"https://*.instagram.com/*",
	"https://instagram.com/*",
	"https://*.twitter.com/*",
	"https://twitter.com/*",
	"https://*.facebook.com/*",
	"https://facebook.com/*",
    	"https://*.reddit.com/*",
    	"https://reddit.com/*",
	"https://*.4chan.org/*",
    	"https://4chan.org/*",
	"https://*.4channel.org/*",
    	"https://boards.4channel.org/*",
	"https://*.8ch.net/*",
    	"https://8ch.net/*",
	"https://*.discordapp.com/*",
    	"https://discordapp.com/*",
    "contextMenus",
    "tabs"
  ],
  "background": {
    "scripts": [
      "open-with-nhentai.js"
    ]
  },
  "web_accessible_resources": [
    "open-with-nhentai.js"
  ],
  "icons": {
    "16": "nh.png",
    "48": "nh.png",
    "128": "nh.png"
  }
}
